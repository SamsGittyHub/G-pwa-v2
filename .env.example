# TripleG Genius Configuration
# Copy this file to .env and fill in your values

# API Base URL for your backend server
# This is used for authentication and database operations
VITE_API_BASE_URL=https://your-backend-api.com

# Auth API URL (optional - defaults to VITE_API_BASE_URL/api/auth)
VITE_AUTH_API_URL=

# Custom AI Model Configuration
# Your custom AI model API endpoint (e.g., http://localhost:8000/v1/chat/completions)
VITE_AI_API_ENDPOINT=

# Your AI model API key (leave empty if not required for local model)
VITE_AI_API_KEY=
